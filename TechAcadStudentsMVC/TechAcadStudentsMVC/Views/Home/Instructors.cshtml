@using TechAcadStudentsMVC.Models
@model List<Instructor> 
    <!--below using statements we define our model at sign model means there is a model followed by its data type
        you can reference the model anywhere using Model-->


@{
    ViewBag.Title = "Instructors";
}

<h2>Instructors</h2>

@{ 
    foreach(Instructor instructor in Model)
    {
        <h4>
            @instructor.FirstName
        </h4>
    }
}